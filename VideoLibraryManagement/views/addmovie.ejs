<form action="/addMovie" method="post">
<div class="col-sm-3 col-sm-offset-3">
<form class="form-horizontal input-sm" action="">


<h3>Add Movie</h3>
<hr>

  <div class="form-group">
  <label>MovieName</label>
  <input type="text" class="form-control input-sm" name="moviename" required="true">
  </div>
 
 <div class="form-group">
  <label>MovieBanner</label>
  <input type="text" class="form-control input-sm" name="moviebanner" required="true">
  </div>
  
 
  
 <div class="form-group">
			<label>Category</label>
			<select class="form-control input-sm" name="category">
			<option value="drama">Drama</option>
			<option value="comedy">Comedy</option>
			<option value="action">Action</option>
			<option value="educational">Educational</option>
			</select>
 </div>

 <div class="form-group">
  <label>Release Date</label>
  <input type="text" class="form-control input-sm" name="releasedate" required="true">
  </div>
  
 
 <div class="form-group">
  <label>Copies</label>
  <input type="text" class="form-control input-sm" name="copies" required="true">
  </div>
  
 
  <div>
  <button type="submit" class="btn btn-info">Add Movie</button>
  </div>
</form>
